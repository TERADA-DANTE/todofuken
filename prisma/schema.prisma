// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Result{
  id          Int @default(autoincrement()) @id
  zipcode     String
  prefecture  String
  city        String
  area        String
  code        Int
}